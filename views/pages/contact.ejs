<%- contentFor('heros') %>
<div class="jumbotron text-center">
    <h1>Pagina de Referencias</h1>
  
    <div class="row">

        <h2>Fuentes</h2>

        <h2>Principales Participantes<h2>
          <%- contentFor('body') %>
              <% for (user of usr) { %>
                <div class="col-sm-3">
                  <h2><%= user.name %></h2>
                  <img src="<%= user.avatar %>" alt="" class="img-responsive">
                <p><%= user.email %></p>
              </div>
              <% } %>
    </div>
  </div>